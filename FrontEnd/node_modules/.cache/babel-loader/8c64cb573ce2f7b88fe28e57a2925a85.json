,IAAA,EAAA,OAAA,IAAA,KAAA,QAAA,CAAA,KACA,IAAA,EAAA,CAAA,IAAA,GAAA,EAAA,CAAA,IAAA,EAAA,OAAA,CAAA,CAAA,GAAA,KAAA,QAAA,CAAA,KACA,OAAA,CAAA;AACA,KAlBA;AAoBA,IAAA,QApBA,oBAoBA,EApBA,EAoBA;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,GAAA,KAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,KAAA,EAAA,IAAA,CAAA,IAAA,CAAA,QADA;AAEA,QAAA,IAAA,EAAA,KAAA,gBAAA,CAAA,IAAA,CAFA;AAGA,QAAA,QAAA,EAAA,OAAA,4BAAA,KAAA,oBAAA,CAAA,IAAA,CAAA,YAAA,CAAA,MAAA,EAAA,MAAA,UAHA;AAIA,QAAA,UAAA,EAAA,GAJA;AAKA,QAAA,WAAA,EAAA,GALA;AAMA,QAAA,QAAA,EAAA;AANA,OAAA;AAQA,KA9BA;AAgCA,IAAA,gBAhCA,4BAgCA,IAhCA,EAgCA;AACA,uCACA,IAAA,CAAA,YAAA,CAAA,MADA,sBACA,KAAA,oBAAA,CAAA,IAAA,CAAA,YAAA,CAAA,MAAA,EAAA,IADA,kCAEA,IAAA,CAAA,WAFA,yDAIA,IAAA,CAAA,IAJA,yDAKA,IAAA,CAAA,IAAA,KAAA,OAAA,GAAA,eAAA,GAAA,mBALA,sDAMA,KAAA,UAAA,CAAA,IAAA,CAAA,iBAAA,CANA,yDAOA,KAAA,UAAA,CAAA,IAAA,CAAA,UAAA,CAPA,4CAQA,IAAA,CAAA,QAAA,CAAA,IARA,6CASA,IAAA,CAAA,QAAA,CAAA,OATA;AAYA,KA7CA;AA8CA,IAAA,UAAA,EAAA,oBAAA,CAAA,EACA;AACA,UAAA,OAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CAAA;AACA,aAAA,OAAA,CAAA,WAAA,KAAA,GAAA,IAAA,OAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,OAAA,CAAA,OAAA,EAAA,GAAA,GAAA,GAAA,OAAA,CAAA,QAAA,EAAA,GAAA,GAAA,GAAA,OAAA,CAAA,UAAA,EAAA,GAAA,GAAA,GAAA,OAAA,CAAA,UAAA,EAAA;AACA,KAlDA;AAmDA,IAAA,UAnDA,sBAmDA,EAnDA,EAmDA;AAAA;;AACA,WAAA,QAAA,CACA,sBADA,EAEA,oCAFA,EAGA,SAHA,EAIA;AAAA,QAAA,iBAAA,EAAA,IAAA;AAAA,QAAA,gBAAA,EAAA;AAAA,OAJA,EAKA,IALA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,gBAAA,WAAA,GAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,MAAA,CACA,MAAA,CAAA,UADA,EAEA,sBAFA,EAGA,SAHA;;AAKA,UAAA,MAAA,CAAA,UAAA;AACA,SAPA;AAQA,OAfA,EAgBA,YAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,YAAA,EAAA,MAAA;AACA,OAlBA;AAoBA;AAxEA,GAfA;AAyFA,EAAA,OAzFA,qBAyFA;AACA,SAAA,UAAA;AACA;AA3FA,CAAA","sourcesContent":["<template>\n  <section class=\"page-section\">\n    <b-container>\n     <HeaderPage title=\"Gestão de Utilizadores\"/>\n      \n      <!--MENU TOPO-->\n      <b-row class=\"mb-4\">\n        <b-col cols=\"1\"></b-col>\n        <b-col>\n          <router-link\n            :to=\"{name:'addUser'}\"\n            tag=\"button\"\n            class=\"btn btn-outline-success mr-2 mt-2\"\n          ><i class=\"fas fa-plus-square\"></i> ADICIONAR UTILIZADOR</router-link>\n          <router-link\n            :to=\"{name:'admin'}\"\n            tag=\"button\"\n            class=\"btn btn-outline-info mr-2 mt-2\"\n          ><i class=\"fas fa-bars\"></i> MENU PRINCIPAL</router-link>\n        </b-col>\n        <b-col cols=\"1\"></b-col>\n      </b-row>\n\n      <!-- TABELA -->\n      <b-row>\n        <b-col cols=\"1\"></b-col>\n        <b-col>\n          <table class=\"table table-striped\">\n            <thead class=\"thead-dark\">\n              <tr>\n                <th scope=\"col\">\n                  NOME\n                  <i class=\"fas fa-arrow-up\" v-if=\"sortType===1\" @click=\"sort()\"></i>\n                  <i class=\"fas fa-arrow-down\" v-else  @click=\"sort()\"></i>\n                </th>\n                <th scope=\"col\">TIPO</th>\n                <th scope=\"col\">DATA DE CRIAÇÃO</th>\n                <th scope=\"col\">AÇÕES</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"user of users\" :key=\"user._id\">                \n                <td class=\"pt-4\">{{user.name}}</td>\n                <td class=\"pt-4\">{{user.type===\"admin\"?\"Administrador\":\"Utilizador normal\"}}</td>\n                <td class=\"pt-4\">{{formatDate(user.registration_date)}}</td>\n                <td>\n                  <router-link\n                    :to=\"{name:'editUser', params:{userId: user._id}}\"\n                    tag=\"button\"\n                    class=\"btn btn-outline-success mr-2\"\n                  ><i class=\"fas fa-edit\"></i> EDITAR</router-link>\n                  <button\n                    @click=\"viewUser(user._id)\"\n                    type=\"button\"\n                    class=\"btn btn-outline-success mr-2\"\n                  ><i class=\"fas fa-search\"></i> VER</button>\n                  <button\n                    @click=\"removeUser(user._id)\"\n                    type=\"button\"\n                    class=\"btn btn-outline-danger mr-2 \"\n                  ><i class=\"fas fa-trash-alt\"></i> REMOVER</button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </b-col>\n        <b-col cols=\"1\"></b-col>\n      </b-row>\n    </b-container>\n  </section>\n</template>\n\n<script>\nimport { FETCH_USERS, REMOVE_USER } from \"@/store/users/user.constants\";\nimport HeaderPage from \"@/components/HeaderPage.vue\"\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"ManageUsers\",\n  components: {\n    HeaderPage\n  },\n  data: function() {\n    return {\n      users: [],\n      sortType: 1\n    };\n  },\n  computed: {    \n    ...mapGetters([\"getUserLevelByPoints\"]),\n    ...mapGetters(\"user\", [\"getUsers\",\"getMessage\"])\n  },\n  methods: {\n    fetchUsers() {\n      this.$store.dispatch(`user/${FETCH_USERS}`).then( \n        () => {\n          this.users = this.getUsers;\n        }, err => {\n          this.$alert(`${err.message}`, 'Erro', 'error');\n        });\n    },\n    sort() {\n      this.users.sort(this.compareNames)\n      this.sortType *= -1\n      \n    },\n    compareNames(u1,u2) {\n      if(u1.name > u2.name) return 1 * this.sortType\n      else if(u1.name < u2.name) return -1 * this.sortType\n      else return 0\n    },\n\n    viewUser(id) {\n      const user = this.users.find(user => user._id === id);\n      this.$fire({\n        title: user.auth.username,\n        html: this.generateTemplate(user),\n        imageUrl: require(`@/assets/avatars/${this.getUserLevelByPoints(user.gamification.points).avatar}.png`),\n        imageWidth: 150,\n        imageHeight: 150,\n        imageAlt: \"Imagem desconhecida\"\n      });\n    },\n\n    generateTemplate(user) {\n      return `\n          <h5>${user.gamification.points} pontos (${this.getUserLevelByPoints(user.gamification.points).name})</h5>\n          <p>${user.description}</p>\n          <p>\n          <b>Nome:</b> ${user.name} <br>\n          <b>Tipo de utilizador:</b> ${user.type===\"admin\"?\"Administrador\":\"Utilizador normal\"} <br>\n          <b>Data de registo:</b> ${this.formatDate(user.registration_date)} <br>\n          <b>Data de nascimento:</b> ${this.formatDate(user.birth_date)}<br>\n          <b>Cidade:</b> ${user.location.city}<br>\n          <b>País:</b> ${user.location.country}\n          </p>\n        `\n    },\n    formatDate: d => \n    {\n      const newDate = new Date(Date.parse(d))\n      return newDate.getFullYear() + \"-\" + (newDate.getMonth() + 1) + \"-\" + newDate.getDate() + \" \" + newDate.getHours() + \":\" + newDate.getMinutes() + \":\" + newDate.getSeconds()\n    },\n    removeUser(id) {\n      this.$confirm(\n        \"Se sim, clique em OK\",\n        \"Deseja mesmo remover o utilizador?\",\n        \"warning\",\n        { confirmButtonText: \"OK\", cancelButtonText: \"Cancelar\" }\n      ).then(\n        () => {\n          this.$store.dispatch(`user/${REMOVE_USER}`, id).then(() => {\n            this.$alert(\n              this.getMessage,\n              \"Utilizador removido!\",\n              \"success\"\n            );\n            this.fetchUsers();\n          });\n        },\n        () => {\n          this.$alert(\"Remoção cancelada!\", \"Informação\", \"info\");\n        }\n      );\n    }\n  },\n  created() {\n    this.fetchUsers();    \n  }\n};\n</script>"],"sourceRoot":"src/views/users"}]}