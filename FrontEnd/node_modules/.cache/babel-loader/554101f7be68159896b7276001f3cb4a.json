A,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAJA,EAKA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,WAAA,GAAA,CAAA,OAAA,GAAA,MAAA,EAAA,OAAA;AACA,OAPA;AASA;AAXA;AAxBA,CAAA","sourcesContent":["<template>\n  <section class=\"page-section\">\n    <b-container>\n      <HeaderPage title=\"Adicionar Patrocinador\" />\n      <b-row>\n        <b-col cols=\"2\"></b-col>\n        <b-col>\n          <form @submit.prevent=\"add\">\n            <div class=\"form-group\">\n              <input\n                v-model=\"img\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Imagem do Patrocinador\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"name\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Nome do Patrocinador\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"animal\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Animal\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <select\n                id=\"sltGroup\"\n                class=\"form-control form-control-lg\"\n                v-model=\"group\"\n                required\n              >\n                <option value>-- SELECIONA GRUPO --</option>\n                <option value=\"anfibio\">ANFÍBIO</option>\n                <option value=\"ave\">AVE</option>\n                <option value=\"mamifero\">MAMÍFERO</option>\n                <option value=\"peixe\">PEIXE</option>\n                <option value=\"reptil\">RÉPTIL</option>\n              </select>\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"years_sponsored\"\n                type=\"number\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Anos de Patrocínio\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <textarea\n                v-model=\"description\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Descrição\"\n                rows=\"5\"\n                required\n              ></textarea>\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"location.city\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Cidade\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"location.country\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"País\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"value\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Valor por ano\"\n                required\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                v-model=\"contact\"\n                type=\"text\"\n                class=\"form-control form-control-lg\"\n                placeholder=\"Contacto\"\n                required\n              />\n            </div>\n            <button type=\"submit\" class=\"btn btn-outline-success btn-lg mr-2\">\n              <i class=\"fas fa-plus-square\"></i> ADICIONAR\n            </button>\n            <router-link\n              :to=\"{ name: 'listSponsors' }\"\n              tag=\"button\"\n              class=\"btn btn-outline-danger btn-lg\"\n            >\n              <i class=\"fas fa-window-close\"></i> CANCELAR\n            </router-link>\n          </form>\n        </b-col>\n        <b-col cols=\"2\"></b-col>\n      </b-row>\n    </b-container>\n  </section>\n</template>\n\n<script>\nimport { ADD_SPONSOR } from \"@/store/sponsors/sponsor.constants\";\nimport HeaderPage from \"@/components/HeaderPage.vue\";\nimport router from \"@/router\";\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AddSponsor\",\n  components: {\n    HeaderPage\n  },\n  data: () => {\n    return {\n      img: \"\",\n      name: \"\",\n      animal: \"\",\n      group: \"\",\n      years_sponsored: 0,\n      description: \"\",\n      contact: \"\",\n      value: 0.0,\n      location: {\n        city: \"\",\n        country: \"\"\n      }\n    };\n  },\n  computed: {\n    ...mapGetters(\"sponsor\", [\"getMessage\"])\n  },\n  methods: {\n    add() {\n      this.$store.dispatch(`sponsor/${ADD_SPONSOR}`, this.$data).then(\n        () => {\n          this.$alert(this.getMessage, \"Sponsor adicionado!\", \"success\");\n          router.push({ name: \"listSponsors\" });\n        },\n        err => {\n          this.$alert(`${err.message}`, \"Erro\", \"error\");\n        }\n      );\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/sponsors"}]}